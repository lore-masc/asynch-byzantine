<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />
	<parameter name="processes_count" displayName="Number of processes" type="int" 
						defaultValue="35" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"						
		/>
	<parameter name="min_workload" displayName="Workload for node (Min)" type="int" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
	<parameter name="max_workload" displayName="Workload for node (Max)" type="int" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
	<parameter name="send_broadcast" displayName="Probabilityto send new broadcast" type="double" 
						defaultValue="1000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
</parameters>
